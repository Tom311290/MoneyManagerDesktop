<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>


<SplitPane dividerPositions="0.9350348027842227" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="moneyManager.viewControllers.ExpensesTableOverviewController">
   <items>
      <TableView fx:id="tableExpenses">
         <columns>
            <TableColumn fx:id="tableExpensesColumnCost" prefWidth="75.0" text="Cost" />
            <TableColumn fx:id="tableExpensesColumnCurrency" prefWidth="75.0" text="Currency" />
            <TableColumn fx:id="tableExpensesColumnCategory" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="tableExpensesColumnNote" prefWidth="75.0" text="Note" />
            <TableColumn fx:id="tableExpensesColumnExpenseDate" maxWidth="1.7976931348623157E308" prefWidth="75.0" text="Expense date" />
            <TableColumn fx:id="tableExpensesColumnInputDate" maxWidth="1.7976931348623157E308" prefWidth="75.0" text="Input date" />
         </columns>
      </TableView>
      <ButtonBar maxWidth="1.7976931348623157E308">
         <buttons>
            <Button fx:id="refreshTable" maxHeight="25.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#initializeTableExpenses" text="Refresh" textAlignment="CENTER" />
         </buttons>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ButtonBar>
   </items>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</SplitPane>
